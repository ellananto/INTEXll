@{
}
@model BurialsViewModel

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Structure ID</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var b in Model.Structure)
        {
            <tr>
                <td>@b.Id</td>
                <td>@b.Structureid</td>
                <td>@b.Value</td>
            </tr>
        }
    </tbody>
</table>