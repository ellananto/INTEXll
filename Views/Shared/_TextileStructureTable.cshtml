@{
}
@model BurialsViewModel
<!--Textile Structure Table-->
<table class="table text-white" style="background: rgba(0, 0, 0, 0.4);">
    <thead>
        <tr>
            <th>ID</th>
            <th>Structure ID</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var b in Model.Structure)
        {
            <tr>
                <td>@b.Id</td>
                <td>@b.Structureid</td>
                <td>@b.TSValue</td>
            </tr>
        }
    </tbody>
</table>