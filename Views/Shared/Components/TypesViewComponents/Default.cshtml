@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Burials" asp-route-Area="" asp-route-pageNum="1" class="btn btn-outline-secondary">Home</a>

    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Burials" asp-route-Area="@t"
           class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
    }
</div>